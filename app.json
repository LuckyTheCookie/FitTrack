{
  "expo": {
    "name": "FitTrack",
    "slug": "fittrack-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "scheme": "fittrack",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#0b0c0f"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.fittrack.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0b0c0f"
      },
      "package": "com.fittrack.app",
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      [
        "react-native-vision-camera",
        {
          "cameraPermissionText": "$(PRODUCT_NAME) a besoin de la caméra pour détecter vos mouvements",
          "enableFrameProcessors": true
        }
      ],
      "./plugins/withMediaPipeModel.js"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
